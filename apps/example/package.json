{
  "private": true,
  "name": "@acme/app-example",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "lint": "eslint --ext js,ts,tsx .",
    "test": "jest",
    "build": "expo export --output-dir ./build --platform all",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eas-build-post-install": "pnpm run -w build:example"
  },
  "dependencies": {
    "@acme/feature-home": "workspace:*",
    "@expo/vector-icons": "^15.0.0",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "expo": "54.0.0-preview.9",
    "expo-blur": "~15.0.3",
    "expo-constants": "~18.0.4",
    "expo-font": "~14.0.4",
    "expo-haptics": "~15.0.3",
    "expo-linking": "~8.0.4",
    "expo-router": "6.0.0-beta.8",
    "expo-splash-screen": "~31.0.5",
    "expo-status-bar": "~3.0.4",
    "expo-symbols": "~1.0.3",
    "expo-system-ui": "~6.0.3",
    "expo-web-browser": "~15.0.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.0.3",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.15.3",
    "react-native-web": "~0.21.1",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.12",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.4",
    "typescript": "^5.9.2"
  },
  "eslintConfig": {
    "extends": "@acme/eslint-config",
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  },
  "jest": {
    "preset": "jest-expo"
  }
}
