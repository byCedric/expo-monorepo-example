{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": false,
    "strict": false,
    "jsx": "react",
    "module": "esnext",
    "target": "esnext",
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "build/dist",
  },
}
